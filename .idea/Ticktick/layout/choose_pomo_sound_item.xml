<?xml version="1.0" encoding="utf-8"?>
<com.ticktick.task.view.selectableview.SelectableRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:gravity="center_vertical" android:layout_gravity="bottom" android:paddingLeft="24dp" android:layout_width="match_parent" android:layout_height="48dp">
        <FrameLayout android:id="@+id/left_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true">
            <com.ticktick.task.view.IconTextView android:textSize="24sp" android:textColor="?android:attr/textColorSecondary" android:gravity="center" android:id="@+id/left_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/ic_svg_normal_list"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/ag9" android:singleLine="true" android:layout_toLeftOf="@+id/right_layout" android:layout_toRightOf="@+id/left_layout" android:layout_alignWithParentIfMissing="true" android:marqueeRepeatLimit="10" style="@style/dialog_text_style"/>
        <LinearLayout android:gravity="center" android:id="@+id/right_layout" android:layout_width="48dp" android:layout_height="match_parent" android:layout_marginRight="16dp" android:layout_alignParentRight="true">
            <androidx.appcompat.widget.AppCompatRadioButton android:id="@+id/selection_icon" android:background="@null" android:focusable="false" android:visibility="gone" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="center"/>
            <com.ticktick.task.view.IconTextView android:textSize="24dp" android:textColor="?android:attr/textColorSecondary" android:id="@+id/selection_lock_icon" android:background="@null" android:focusable="false" android:visibility="gone" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scaleType="center" android:text="@string/ic_svg_sound_lock"/>
        </LinearLayout>
    </RelativeLayout>
</com.ticktick.task.view.selectableview.SelectableRelativeLayout>
