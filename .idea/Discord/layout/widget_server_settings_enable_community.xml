        <com.google.android.material.button.MaterialButton android:ellipsize="end" android:id="@+id/community_setting_get_started" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" android:text="@g/get_started" android:maxLines="1" style="@h/UiKit_Material_Button"/>
        <TextView android:gravity="center" android:padding="0dp" android:layout_marginLeft="16dp" android:layout_marginTop="24dp" android:layout_marginRight="16dp" android:layout_marginBottom="40dp" android:text="@g/guild_settings_community_intro_details_mobile" style="@h/UiKit_Settings_Item_SubText"/>
        <com.discord.widgets.servers.community.CommunityGetStartedInformationView android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" app:csi_description_text="@g/guild_settings_community_intro_upsell_discovery_body" app:csi_icon="?attr/drawable_ic_community_setting_analytics" app:csi_title_text="@g/guild_settings_community_intro_upsell_discovery_header"/>
        <com.discord.widgets.servers.community.CommunityGetStartedInformationView android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="10dp" android:layout_marginRight="16dp" android:layout_marginBottom="10dp" app:csi_description_text="@g/guild_settings_community_intro_upsell_analytics_body_mobile" app:csi_icon="?attr/drawable_ic_community_setting_server_insights" app:csi_title_text="@g/guild_settings_community_intro_upsell_analytics_header"/>
