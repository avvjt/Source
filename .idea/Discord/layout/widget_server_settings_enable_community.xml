<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/community_get_started_scroll" style="@h/UiKit_ViewGroup_NestedScrollView_AppBarScrollingViewBehavior">
