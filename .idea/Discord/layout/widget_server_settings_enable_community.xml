            <com.discord.widgets.servers.community.CommunityGetStartedHeaderIconView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:layout_marginEnd="16dp" android:layout_alignEnd="@+id/community_get_started_header_image" app:chi_icon="@d/community_server_get_started_sneakers" app:chi_title_text="Sneaker Fans"/>
