<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" style="@h/UiKit_ViewGroup_Page">
    <com.google.android.material.appbar.AppBarLayout style="@h/AppTheme_AppBarLayout_Flat">
        <androidx.appcompat.widget.Toolbar style="@h/AppTheme_Toolbar"/>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.core.widget.NestedScrollView style="@h/UiKit_ViewGroup_NestedScrollView_AppBarScrollingViewBehavior">
        <LinearLayout style="@h/UiKit_ViewGroup_LinearLayout">
            <TextView android:text="@g/form_label_verification_level" style="@h/UiKit_Settings_Item_Header"/>
            <TextView android:id="@+id/server_settings_moderation_verification_help" android:paddingTop="0dp" style="@h/UiKit_Settings_Item"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_verification_1" app:cs_subtext="@g/verification_level_none_criteria" app:cs_text="@g/verification_level_none" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_verification_2" app:cs_subtext="@g/verification_level_low_criteria" app:cs_text="@g/verification_level_low" app:cs_text_color="@c/status_green_500" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_verification_3" app:cs_subtext="@g/verification_level_medium_criteria" app:cs_text="@g/verification_level_medium" app:cs_text_color="@c/status_yellow_500" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_verification_4" app:cs_subtext="@g/verification_level_high_criteria" app:cs_text="(╯°□°）╯︵ ┻━━┻" app:cs_text_color="@c/status_orange_500" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_verification_5" app:cs_subtext="@g/verification_level_very_high_criteria" app:cs_text="┻━┻ ﾐヽ(ಠ益ಠ)ノ彡┻━┻" app:cs_text_color="@c/status_red_500" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <View style="@h/UiKit_Settings_Divider"/>
            <TextView android:text="@g/form_label_explicit_content_filter" style="@h/UiKit_Settings_Item_Header"/>
            <TextView android:id="@+id/server_settings_moderation_explicit_help" android:paddingTop="0dp" style="@h/UiKit_Settings_Item"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_explicit_1" app:cs_subtext="@g/explicit_content_filter_disabled_description" app:cs_text="@g/explicit_content_filter_disabled" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_explicit_2" android:layout_height="wrap_content" app:cs_subtext="@g/explicit_content_filter_medium_description" app:cs_text="@g/explicit_content_filter_medium" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <com.discord.views.CheckedSetting android:id="@+id/server_settings_moderation_explicit_3" app:cs_subtext="@g/explicit_content_filter_high_description" app:cs_text="@g/explicit_content_filter_high" app:cs_view_type="1" style="@h/UiKit_CheckedSetting_Themed"/>
            <View style="@h/UiKit_Settings_Divider"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
