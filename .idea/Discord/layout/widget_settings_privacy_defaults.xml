<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" style="@h/AppAlertDialog">
    <TextView android:text="@g/user_dm_settings" style="@h/AppAlertDialogHeader"/>
    <LinearLayout style="@h/AppAlertDialogContent">
        <TextView android:text="@g/user_dm_settings_question" style="@h/AppAlertDialogText"/>
    </LinearLayout>
    <LinearLayout style="@h/AppAlertDialogFooter">
        <com.google.android.material.button.MaterialButton android:id="@+id/settings_privacy_defaults_existing_no" android:text="@g/no_text" style="@h/UiKit_Material_Button_Transparent_Fit"/>
        <com.google.android.material.button.MaterialButton android:id="@+id/settings_privacy_defaults_existing_yes" android:text="@g/yes_text" style="@h/UiKit_Material_Button_Red_Fit"/>
    </LinearLayout>
</LinearLayout>
