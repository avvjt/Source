<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/community_first_step_scroll" android:layout_width="match_parent" style="@h/UiKit_ViewGroup_NestedScrollView_AppBarScrollingViewBehavior">
    <LinearLayout android:focusableInTouchMode="true" style="@h/UiKit_ViewGroup_LinearLayout">
        <ImageView android:id="@+id/community_get_started_header_image" android:layout_width="match_parent" android:layout_height="wrap_content" android:src="?attr/drawable_community_setup_third_step" android:importantForAccessibility="no"/>
        <com.discord.views.ScreenTitleView android:id="@+id/community_step_header" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="40dp" android:layout_marginRight="16dp" android:layout_marginBottom="24dp" app:stv_subtitle="@g/enable_community_modal_step_3_body" app:stv_title="@g/enable_community_modal_step_3_title"/>
        <com.discord.views.CheckedSetting android:id="@+id/community_setting_notifications_to_mentions_switch" app:cs_text="@g/enable_community_modal_default_notifications_label_mobile" app:cs_text_color="?attr/primary_000" app:cs_text_fontpath="@font/whitney_semibold" app:cs_view_type="2" style="@h/UiKit_CheckedSetting_Themed"/>
        <View style="@h/UiKit_Settings_Divider"/>
        <com.discord.views.CheckedSetting android:id="@+id/community_setting_manage_permissions_switch" app:cs_text_color="?attr/primary_000" app:cs_text_fontpath="@font/whitney_semibold" app:cs_view_type="2" style="@h/UiKit_CheckedSetting_Themed"/>
        <View style="@h/UiKit_Settings_Divider"/>
        <TextView android:textSize="@dimen/uikit_textsize_small" android:textColor="?attr/colorHeaderSecondary" android:paddingLeft="16dp" android:paddingTop="16dp" android:paddingRight="16dp" android:layout_width="match_parent" android:text="@g/public_guild_policy_title" android:textAllCaps="true" style="@h/UiKit_TextView_Bold"/>
        <com.discord.views.CheckedSetting android:id="@+id/community_setting_community_guidelines_switch" app:cs_text="@g/public_guild_policy_accept" app:cs_text_color="?attr/primary_000" app:cs_text_fontpath="@font/whitney_semibold" app:cs_view_type="0" style="@h/UiKit_CheckedSetting_Themed"/>
        <com.discord.utilities.view.text.LinkifiedTextView android:textColor="?attr/colorHeaderSecondary" android:id="@+id/community_setting_guidelines" android:paddingLeft="16dp" android:paddingRight="16dp" android:layout_marginBottom="16dp" style="@h/UiKit_TextView"/>
    </LinearLayout>
</ScrollView>
