<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <FrameLayout android:gravity="center" android:id="@+id/guilds_item_avatar_wrap" android:layout_width="@dimen/avatar_size_large" android:layout_height="@dimen/avatar_size_large" android:layout_centerInParent="true">
        <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/guilds_item_avatar" app:fadeDuration="50" style="@h/UiKit_GuildsListIcon"/>
        <TextView android:textColor="?attr/colorHeaderPrimary" android:gravity="center" android:id="@+id/guilds_item_avatar_text" android:clickable="false" android:layout_width="match_parent" android:layout_height="match_parent" android:singleLine="true" android:importantForAccessibility="no" style="@h/UiKit_TextView"/>
    </FrameLayout>
    <ImageView android:id="@+id/guilds_item_application_stream" android:visibility="gone" android:layout_alignTop="@+id/guilds_item_avatar_wrap" android:contentDescription="@g/status_streaming" android:layout_alignEnd="@+id/guilds_item_avatar_wrap" style="@h/UiKit_GuildsListApplicationStreamIcon"/>
    <ImageView android:id="@+id/guilds_item_voice" android:layout_alignTop="@+id/guilds_item_avatar_wrap" android:contentDescription="@g/connection_status_voice_connected" android:layout_alignEnd="@+id/guilds_item_avatar_wrap" style="@h/UiKit_GuildsListVoiceIcon"/>
    <TextView android:id="@+id/guilds_item_mentions" android:layout_alignBottom="@+id/guilds_item_avatar_wrap" android:layout_alignEnd="@+id/guilds_item_avatar_wrap" style="@h/UiKit_GuildsListMentions"/>
    <ImageView android:id="@+id/guilds_item_application_status" android:background="@d/drawable_circle_black" android:visibility="gone" android:layout_alignBottom="@+id/guilds_item_avatar_wrap" android:contentDescription="@g/member_verification_icon_desc" android:layout_alignEnd="@+id/guilds_item_avatar_wrap" app:srcCompat="@d/ic_application_status_rejected" style="@h/UiKit_GuildsListApplicationStatus"/>
</merge>
