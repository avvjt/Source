<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" style="@h/UiKit_ViewGroup_ScrollView_Page">
    <LinearLayout android:paddingBottom="16dp" style="@h/UiKit_ViewGroup_LinearLayout">
        <androidx.constraintlayout.widget.ConstraintLayout style="@h/UiKit_Sheet_Header">
            <androidx.constraintlayout.widget.Guideline style="@h/UiKit_Sheet_Guideline"/>
            <TextView android:id="@+id/guild_actions_overview_header_tv" android:text="@g/select_sort_mode" style="@h/UiKit_Sheet_Header_Title"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View style="@h/UiKit_Settings_Divider"/>
        <TextView android:id="@+id/server_settings_channels_sort_actions_category" android:text="@g/category" app:drawableStartCompat="?attr/ic_category" style="@h/UiKit_Settings_Item_Icon"/>
        <TextView android:id="@+id/server_settings_channels_sort_actions_voice" android:text="@g/voice_channel" app:drawableStartCompat="?attr/ic_volume_up" style="@h/UiKit_Settings_Item_Icon"/>
        <TextView android:id="@+id/server_settings_channels_sort_actions_text" android:text="@g/text_channel" app:drawableStartCompat="?attr/ic_channel_text" style="@h/UiKit_Settings_Item_Icon"/>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
