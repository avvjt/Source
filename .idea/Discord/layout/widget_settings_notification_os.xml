<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="?attr/primary_630" style="@h/UiKit_ViewGroup_LinearLayout">
    <com.discord.views.CheckedSetting android:id="@+id/settings_notification_switch" app:cs_subtext="@g/form_label_mobile_notifications_system_desc" app:cs_text="@g/form_label_mobile_notifications_system_label" app:cs_view_type="2" style="@h/UiKit_CheckedSetting_Themed"/>
