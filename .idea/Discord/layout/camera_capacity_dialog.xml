<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" style="@h/UiKit_ViewGroup_Page_LinearLayout">
    <LinearLayout android:id="@+id/notice_header_container" style="@h/UiKit_ViewGroup_LinearLayout">
        <TextView android:padding="16dp" android:text="@g/video_capacity_modal_header" style="@h/UiKit_TextView_H1_Bold"/>
        <View style="@h/UiKit_Divider"/>
    </LinearLayout>
    <TextView android:textColor="@c/primary_200" android:id="@+id/camera_capacity_body" android:paddingTop="8dp" android:layout_marginBottom="8dp" android:text="@g/video_capacity_modal_body" android:paddingStart="16dp" android:paddingEnd="16dp" style="@h/UiKit_TextView"/>
    <View style="@h/UiKit_Settings_Divider"/>
    <LinearLayout style="@h/UiKit_Dialog_ButtonContainer">
        <com.google.android.material.button.MaterialButton android:textColor="@c/white" android:layout_gravity="center_vertical" android:id="@+id/camera_capacity_confirm" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@g/okay" style="@h/UiKit_Material_Button"/>
    </LinearLayout>
</LinearLayout>
