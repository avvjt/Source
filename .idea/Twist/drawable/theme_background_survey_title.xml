        <shape android:shape="oval">
            <size android:height="96dp" android:width="96dp"/>
            <stroke android:width="1dp" android:color="@color/theme_survey_background_overlay"/>
        </shape>
    </item>
    <item android:gravity="center" android:left="16dp" android:top="16dp" android:right="16dp" android:bottom="16dp">
        <shape android:shape="oval">
            <size android:height="64dp" android:width="64dp"/>
            <solid android:color="@color/theme_survey_background_overlay"/>
        </shape>
    </item>
    <item android:gravity="center" android:drawable="?attr/themeSurveyTitleIcon" android:left="32dp" android:top="32dp" android:right="32dp" android:bottom="32dp"/>
</layer-list>
